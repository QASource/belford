package com.qa.scripts;

import org.testng.Assert;
import org.testng.annotations.Test;

import com.qa.Functions.common.CommonUtils;
import com.qa.Functions.webdriver.UIEvents;
import com.qa.ReusableActions.ReusableActions;

public class TC_29_RefineClose extends SetupScript{
	
	@Test(description="TC_29_Verify that User can close the Refine dialog box by cross button", groups="SmokeTest")	
	
	public void refinecross() throws Exception
	{

		ReusableActions.accountSignIn(driver, CommonUtils.readIni("TestData.ini", "txt_SingInUserName"), CommonUtils.readIni("TestData.ini", "txt_SingInPassword"));
		
		UIEvents.click(driver, CommonUtils.readIni("Repository.ini", "btn_Refine"));
		
		UIEvents.click(driver,CommonUtils.readIni("Repository.ini", "btn_refine_close"));
		
		Assert.assertNotNull(UIEvents.waitForElement(driver, CommonUtils.readIni("Repository.ini", "hd_DealsListPage")));
	}

}
