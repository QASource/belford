package com.qa.scripts;

import org.testng.Assert;
import org.testng.annotations.Test;

import com.qa.Functions.common.CommonUtils;
import com.qa.Functions.webdriver.UIEvents;

public class TC_32_SignUpHelp extends SetupScript {
		
		
		@Test(description="TC_32_Verify that user is navigated to Help page by clicking 'Help' button.", groups="SmokeTest")	
		public void HelpSignUp() throws Exception {
		
		//System is clicking on Signin/Signup link
		UIEvents.click(driver, CommonUtils.readIni("Repository.ini", "lnk_Login_Singup"));
		
		//System is clicking on Signin/Signup link
		UIEvents.click(driver, CommonUtils.readIni("Repository.ini", "Help_Singup"));
		
		//System is ensuring whether the user got signed up or not
		Assert.assertNotNull(CommonUtils.readIni("Repository.ini","HelpConfirmationMessage"));
	}		
}



