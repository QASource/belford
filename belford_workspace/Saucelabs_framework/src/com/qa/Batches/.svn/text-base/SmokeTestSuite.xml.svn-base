<suite name="Torbert Smoke Test Execution" verbose="10">
    <listeners > 
      <listener class-name="org.uncommons.reportng.HTMLReporter" /> 
    </listeners>
	<test name="TC_01_Verify that new user is unable to signup in application without specifing city." preserve-order="true">
		<classes>
		  <class name="com.qa.scripts.TC_01_AccountSignupwithoutCity">
			<methods>
				<include name="accountSignupwithoutCity"/>
			</methods>
		  </class>
		</classes>
    </test>
